{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "android",
            "request": "launch",
            "name": "Launch Debug",
            "appSrcRoot": "${workspaceFolder}/app/src/main",
            "apkFile": "${workspaceFolder}/app/build/outputs/apk/debug/app-debug.apk",
            "adbPort": 5037,
            "preLaunchTask": "Build and Install Debug"
        },
        {
            "type": "android",
            "request": "launch",
            "name": "Launch Release",
            "appSrcRoot": "${workspaceFolder}/app/src/main",
            "apkFile": "${workspaceFolder}/app/build/outputs/apk/release/app-release.apk",
            "adbPort": 5037,
            "preLaunchTask": "Build and Install Release"
        },
        {
            "type": "android",
            "request": "attach",
            "name": "Attach to Android Process",
            "appSrcRoot": "${workspaceFolder}/app/src/main",
            "adbPort": 5037,
            "processId": "${command:PickAndroidProcess}"
        }
    ]
}
